{
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "single"]
  },
  "linterOptions": {
    "exclude": ["./node_modules/**"]
  },
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "plugins": [{ "name": "typescript-tslint-plugin" }]
}
